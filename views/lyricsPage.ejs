<!-- Params

songid
url
full_title

-->

<!DOCTYPE html>
<html>
  <head>
    <title>Genius Lyrics for <%= full_title %></title>
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
  </head>
  <body>
    <div
      id="rg_embed_link_<%= songid %>"
      class="rg_embed_link"
      data-song-id="<%= songid %>"
    >
      Read
      <a href="<%= url %>"><%= full_title %></a>
      on Genius
    </div>
    <script
      crossorigin
      src="//genius.com/songs/<%= songid %>/embed.js"
    ></script>
  </body>
</html>
